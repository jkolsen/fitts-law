    "googleapis": "^27.0.0",
    "lodash": "^4.17.11",
    "mongodb": "^3.1.10",
    "ng-google-sheets-db": "^2.0.0",
    "reflect-metadata": "^0.1.10",
    "rxjs": "6.3.3",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.11.4",
    "@angular/cli": "7.1.4",
